<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.juanpickselov.pragprog.persistence.TipMapper">

	<cache />

	<select id="getAllTips" resultType="Tip">
		SELECT TIP_ID AS tipId, TIP AS tip, tip_description AS tipDescription
		FROM PRAGPROG.TIP_LIST
	</select>
</mapper>